name = 'MED'
version = '3.0.6'

homepage = 'http://www.salome-platform.org'
description = """MED data model provides a standardized representation of meshes and fields results."""

toolchain = {'name': 'GCC', 'version': '4.7.2'}

source_urls = ['(none)']
sources = ['%(namelower)s-%(version)s.tar.gz']

dependencies = [
    ('HDF5', '1.8.12', '-serial'),
]

configopts = '--with-hdf5=$EBROOTHDF5 --with-med_int=int'

moduleclass = 'data'
