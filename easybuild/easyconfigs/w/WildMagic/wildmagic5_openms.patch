+
+project("WildMagic5")
+cmake_minimum_required(VERSION 2.8)
+
+include(CheckCXXCompilerFlag)
+if (NOT WIN32) # we only want fPIC on non-windows systems (fPIC is implicitly true there)
+  CHECK_CXX_COMPILER_FLAG("-fPIC" WITH_FPIC)
