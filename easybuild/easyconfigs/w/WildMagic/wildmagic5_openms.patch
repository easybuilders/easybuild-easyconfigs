+  endif()
+endif()
+
+if(MSVC)
+  set(CMAKE_DEBUG_POSTFIX "d")
+endif()
+
+## define custom include path
+set(WM_INCLUDE_INSTALL "include/WildMagic")
+
+## core
+add_subdirectory(LibCore)
+
+## mathematics
+add_subdirectory(LibMathematics)
