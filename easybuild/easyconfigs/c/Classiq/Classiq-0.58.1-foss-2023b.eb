    ('hatchling', '1.18.0'),
    ('Rust', '1.76.0'),
    ('maturin', '1.5.0', '-Rust-1.76.0'),
]

dependencies = [
    ('Python', '3.11.5'),
    ('SciPy-bundle', '2023.11'),
    ('matplotlib', '3.8.2'),
]

