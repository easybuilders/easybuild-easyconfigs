easyblock = "Toolchain"

name = 'gimpi'
version = '2017b'

homepage = '(none)'
description = """GNU Compiler Collection (GCC) based compiler toolchain with Intel MPI."""

toolchain = SYSTEM

local_comp = ('GCC', '6.4.0-2.28')

dependencies = [
    local_comp,
    ('impi', '2017.3.196', '', local_comp),
]

moduleclass = 'toolchain'
