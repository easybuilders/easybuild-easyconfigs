+add_custom_target(parmed ALL DEPENDS ${STAMP_FILE})
+	
+install(CODE "
+	${FIX_BACKSLASHES_CMD}
+	execute_process(
+    COMMAND \"${PYTHON_EXECUTABLE}\" 
