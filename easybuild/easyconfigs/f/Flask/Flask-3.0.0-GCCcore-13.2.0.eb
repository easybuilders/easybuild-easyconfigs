    }),
    ('asgiref', '3.7.2', {
        'checksums': ['9e0ce3aa93a819ba5b45120216b23878cf6e8525eb3848653452b4192b92afed'],
    }),
    ('blinker', '1.7.0', {
        'checksums': ['e6820ff6fa4e4d1d8e2747c2283749c3f547e4fee112b98555cdcdae32996182'],
    }),
    ('flask', version, {
