    ('protobuf', '24.0'),
]

dependencies = [
    ('Python', '3.11.3'),
    ('SciPy-bundle', '2023.07'),
    ('CUDA', '12.1.1', '', SYSTEM),
    ('cuDNN', '8.9.2.26', '-CUDA-%(cudaver)s', SYSTEM),
    ('protobuf-python', '4.24.0'),
]

moduleclass = 'lib'
