toolchain = {'name': 'gfbf', 'version': '2023a'}

download_instructions = "Manual download from https://developer.nvidia.com/nvidia-tensorrt-8x-download"
sources = ['%(name)s-%(version)s.6.Linux.x86_64-gnu.cuda-12.0.tar.gz']
