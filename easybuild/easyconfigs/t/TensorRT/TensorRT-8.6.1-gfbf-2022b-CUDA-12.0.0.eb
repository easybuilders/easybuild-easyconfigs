    ('protobuf', '23.0'),
]

dependencies = [
    ('Python', '3.10.8'),
    ('SciPy-bundle', '2023.02'),
    ('CUDA', '12.0.0', '', SYSTEM),
    ('cuDNN', '8.8.0.121', '-CUDA-%(cudaver)s', SYSTEM),
    ('protobuf-python', '4.23.0'),
]

moduleclass = 'lib'
