# This is an easyconfig file for EasyBuild, see http://easybuilders.github.io/easybuild
easyblock = "Toolchain"

name = 'gmkl'
version = '2018.1'

homepage = '(none)'
description = """GNU Compiler Collection (GCC) based compiler toolchain,
 alongside Intel MKL."""

toolchain = SYSTEM

local_gccver = '7.3.0'

dependencies = [
    ('GCC', local_gccver),
    ('imkl', '2018.1.163'),
]

hidden = True

moduleclass = 'toolchain'
