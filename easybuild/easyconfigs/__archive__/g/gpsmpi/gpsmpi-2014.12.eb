easyblock = "Toolchain"

name = 'gpsmpi'
version = '2014.12'


homepage = '(none)'
description = """gcc and GFortran based compiler toolchain,
 including Parastation MPICH2 for MPI support."""

toolchain = {'name': 'dummy', 'version': 'dummy'}

mpilib = 'psmpi'
mpiver = '5.1.0-1'


compname = 'GCC'
compver = '4.9.2'

comp = (compname, compver)

dependencies = [
    comp,
    (mpilib, mpiver, '', comp),
]

moduleclass = 'toolchain'
